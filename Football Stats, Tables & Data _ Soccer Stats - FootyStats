.fas { transform: scale(0.95, 0.95); }

.fa-lock { transform: scale(0.8, 0.85); }

article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section { display: block; }

body { line-height: 1.1; font-family: "Source Sans Pro", Roboto, "Open Sans", Helvetica, sans-serif; text-size-adjust: none; font-size: 14px; width: 100%; height: 100%; color: rgb(34, 34, 34); background: url("https://cdn.footystats.org/img/noise-2.png") rgb(245, 245, 245); text-rendering: optimizelegibility; }

* { -webkit-font-smoothing: antialiased; text-rendering: optimizelegibility; }

blockquote, q { quotes: none; }

blockquote::after, blockquote::before, q::after, q::before { content: none; }

.v-align-middle { vertical-align: middle !important; }

ol, ul { list-style: none; }

a, a:active, a:link, a:visited { color: rgb(0, 102, 192); text-decoration: none; }

